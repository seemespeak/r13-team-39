<div id="video_popup" class="modal" tabindex="-1" data-width="950" data-height="600">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h3>
      <%= entry.transcription %>
    </h3>
  </div>
  <div class="modal-body">
    <%= render partial: "videos/entry", locals: {entry: entry} %>
  </div>
</div>
